class Student_record
{
int id;
String name;
Student_record(int i, String n)
{
id=i;
name=n;
}
Student_record()
{}
void display()
{
System.out.println(id+" "+name);
}
public static void main(String args[])
{
Student_record s1=new Student_record(12, "Arushi");
Student_record s2=new Student_record();
s2.id=s1.id;
s2.name=s1.name;
s1.display();
s2.display();
}
}
