class Vehicles
{
int vehicle_no;
String vehicle_name;
Vehicles(int x,String y)                                  //parametrised constructor
{
vehicle_no=x;
vehicle_name=y;
}
void display()
{
System.out.println(vehicle_no+" "+vehicle_name);
}
public static void main(String args[])
{
Vehicles v1=new Vehicles(8942, "bike");
Vehicles v2=new Vehicles(6824, "car");
v1.display();                                            //calling method to display object
v2.display();
}
}
