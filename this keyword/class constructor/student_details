class 	Student
{
int id;
String name, course;
float fees;
Student(int id, String name, String course)
{
this.id=id;
this.name=name;
this.course=course;
}
Student(int id, String name, String course, float fees)
{
this(id, name, course);
this.fees=fees;
}
void display()
{
System.out.println(id+" "+name+" "+course+" "+fees);
}
}
