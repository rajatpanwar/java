class Zoo
{
String name;
int cage_no;
int age;

Zoo(String name, int cage_no, int age)
{
this.name=name;
this.cage_no=cage_no;
this.age=age;
}

void display()
{
System.out.println(name+" "+cage_no+" "+age);
}
}

public class Zoo_details
{
public static void main(String args[])
{
Zoo z1=new Zoo("tiger",423,10);
Zoo z2=new Zoo("deer", 328, 5);
z1.display();
z2.display();
}
}
