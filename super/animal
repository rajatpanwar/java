class Animal
{
String color="brown";
}
class Dog extends Animal
{
String color="black";
void printcolor()
{
System.out.println(color);                             //dog class
System.out.println(super.color);                       //animal class
}
}
class Super
{
public static void main(String args[])
{
Dog d=new Dog();
d.printcolor();
}
}
