class Vehicle
{
void manufacture()
{
System.out.println("vehicle maunfacturing");
}
}
class Bike extends Vehicle
{
void manufacture()
{
System.out.println("bike is being manufactured");
}
void finishing()
{
System.out.println("bikes finishing done");
}
void display()
{
super.manufacture();
finishing();
}
}
class Super2
{
public static void main(String args[])
{
Bike b=new Bike();
b.display();
}
}
